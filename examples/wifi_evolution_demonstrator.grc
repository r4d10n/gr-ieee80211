connections:
# TX Chain - DSSS
- [blocks_message_strobe_0, strobe, ieee80211_ppdu_prefixer_0, in]
- [ieee80211_ppdu_prefixer_0, '0', ieee80211_ppdu_chip_mapper_bc_0, '0']
- [ieee80211_ppdu_chip_mapper_bc_0, '0', blocks_selector_0, '0']

# TX Chain - OFDM
- [blocks_message_strobe_0, strobe, ieee80211_encode_0, app in]
- [ieee80211_encode_0, '0', ieee80211_modulation_0, '0']
- [ieee80211_modulation_0, '0', blocks_selector_0, '1']

# Channel
- [blocks_selector_0, '0', blocks_add_xx_0, '0']
- [blocks_selector_0, '0', qtgui_time_sink_x_0, '0']
- [blocks_selector_0, '0', qtgui_freq_sink_x_0, '0']
- [analog_noise_source_x_0, '0', blocks_add_xx_0, '1']
