options:
  parameters:
    author: GNU Radio / gr-ieee80211
    catch_exceptions: 'True'
    category: '[WiFi Demonstrators]'
    cmake_opt: ''
    comment: 'Complete demonstration of WiFi PHY evolution from 802.11b (1 Mbps) to
      802.11ac (866 Mbps)


      Features:

      - Multi-standard support: 802.11b/a/g/n/ac

      - Real-time statistics and visualization

      - Interactive rate control

      - Channel quality monitoring

      - USRP or loopback operation'
    copyright: ''
    description: WiFi Evolution Demonstrator - All Standards (802.11b/a/g/n/ac)
    gen_cmake: 'On'
    gen_linking: dynamic
    generate_options: qt_gui
    hier_block_src_path: .:.
    id: wifi_evolution_demonstrator
    max_nouts: '0'
    output_language: python
    placement: (0,0)
    qt_qss_theme: ''
    realtime_scheduling: ''
    run: 'True'
    run_command: '{python} -u {filename}'
    run_options: prompt
    sizing_mode: fixed
    thread_safe_setters: ''
    title: WiFi Evolution Demonstrator
    window_size: (2400,1800)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 8]
    rotation: 0
    state: enabled

